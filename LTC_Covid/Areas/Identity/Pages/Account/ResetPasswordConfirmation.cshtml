@page
@model ResetPasswordConfirmationModel
@{
    ViewData["Title"] = "Reset password confirmation";
}

<div class="login-form" method="post">
    <div class="card mb-0">
        <div class="card-body">
            <div class="text-center mb-3">
                <h5 class="mb-0"> Your password has been reset. <br /> Please <a asp-page="./Login">click here to log in</a></h5>
            </div>
        </div>
    </div>
</div>
