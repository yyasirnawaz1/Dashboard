
@{
    ViewData["Title"] = "ViewForms";
    Layout = "~/Views/Shared/Layouts/_DashboardLayout.cshtml";
}
<style>
    .form-check-input {
        position: center;
        margin-top: 0px;
        margin-left: 0px;
    }
</style>
<div class="content">

    <!-- Form inputs -->

    <div class="card">
        <div class="card-header header-elements-inline">
            <h5 class="card-title">Enteries</h5>
            <div class="header-elements">
                <div class="list-icons">
                    <button type="button" id="btnCreateForm" class="btn btn-primary" onclick="HomeView.selectSubscriber();">Add Entry</button>
                    <a class="list-icons-item" data-action="collapse"></a>

                </div>
            </div>
        </div>

        <div class="card-body">
            <br />
            <fieldset class="mb-3">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped table-lg">
                        <thead>
                            <tr>
                                <th class="text-center">Last Name</th>
                                <th class="text-center">First Name</th>
                                <th class="text-center">Form Name</th>
                                <th class="text-center">Date of Last Edit</th>
                                <th class="text-center">Pre-Screen</th>
                                <th class="text-center">In-Person</th>
                                <th class="text-center">Action</th>

                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td class="text-center">Ali</td>
                                <td class="text-center">Ch</td>
                                <td class="text-center">COVID</td>
                                <td class="text-center">03/06/2020</td>
                                <td class="text-center">  <input type="checkbox" name="q92" class="form-check-input" checked></td>
                                <td class="text-center">  <input type="checkbox" name="q92" class="form-check-input" checked></td>
                                <td class="text-center">
                                    <div class="list-icons">
                                        <div class="dropdown show">
                                            <a href="#" class="list-icons-item" data-toggle="dropdown" aria-expanded="true"><i class="icon-menu9"></i></a>
                                            <div class="dropdown-menu dropdown-menu-right" x-placement="top-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(-158px, -145px, 0px);">
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-pencil"></i> Edit</a>
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-file-eye"></i> View</a>
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-envelop2"></i> Archive</a>
                                                <div class="dropdown-divider"></div>
                                            </div>
                                        </div>
                                    </div>
                                </td>

                            </tr>
                            <tr>
                                <td class="text-center">Usman</td>
                                <td class="text-center">Khan</td>
                                <td class="text-center">COVID</td>
                                <td class="text-center">03/06/2020</td>
                                <td class="text-center">
                                    <input type="checkbox" name="q92" class="form-check-input" checked>
                                </td>
                                <td class="text-center">
                                    <input type="checkbox" name="q92" class="form-check-input" checked>
                                </td>
                                <td class="text-center">
                                    <div class="list-icons">
                                        <div class="dropdown show">
                                            <a href="#" class="list-icons-item" data-toggle="dropdown" aria-expanded="true"><i class="icon-menu9"></i></a>
                                            <div class="dropdown-menu dropdown-menu-right" x-placement="top-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(-158px, -145px, 0px);">
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-pencil"></i> Edit</a>
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-file-eye"></i> View</a>
                                                <a href="#" class="dropdown-item" onclick=""><i class="icon-envelop2"></i> Archive</a>
                                                <div class="dropdown-divider"></div>
                                            </div>
                                        </div>
                                    </div>
                                </td>

                            </tr>


                        </tbody>
                    </table>
                </div>











            </fieldset>

        </div>
    </div>
    <!-- /form inputs -->

</div>
<div id="entryModel" class="modal fade" tabindex="-1" data-width="300" data-keyboard="false" role="dialog" aria-labelledby="entryModel" aria-hidden="true" data-backdrop="static">

    <div class="modal-dialog  modal-full">
        <div class="modal-content">
            <div class="modal-header btn-primary" style="padding-bottom: 10px">

                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
            </div>

            <div class="modal-body">
                <br />


                <div class="row">
                    <div class="col-md-12">
                        <div class="form-horizontal" role="form">
                            <br />
                            <div class="form-group">

                                <div class="col-md-3">
                                    <input type="text" class="form-control" id="fname" name="fname">
                                    <br />
                                </div>
                            </div>
                            <div class="form-group">

                                <div class="col-md-3">
                                    <input type="text" class="form-control" id="lname" name="lname">
                                    <br />
                                </div>
                            </div>

                            <hr>
                            <div class="form-group">
                                <div class="col-md-12">
                                    <label class="control-label">If you want to add new [subscriber], </label> <a href="#" onclick="HomeView.newSubscription();">Click Here</a>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="btn btn-danger">Close</button>
                <button type="button" onclick="HomeView.CreateForm();" class="btn btn-primary">Add</button>
            </div>

        </div>
    </div>
</div>
<div id="subscriberModel" class="modal fade" tabindex="-1" data-width="600" data-keyboard="false" role="dialog" aria-labelledby="subscriberModel" aria-hidden="true" data-backdrop="static">

    <div class="modal-dialog  modal-full">
        <div class="modal-content">
            <div class="modal-header btn-primary" style="padding-bottom: 10px">
                <h4 class="modal-title">Subscriber Information</h4>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
            </div>

            <div class="modal-body">



                <div class="row">
                    <div class="col-md-12">
                        <div class="form-horizontal" role="form">
                            <div class="form-group">
                                <label class="col-md-3 control-label">
                                    First Name

                                </label>
                                <div class="col-md-9">
                                    <input type="text" class="form-control" id="fname" name="fname">
                                </div>
                            </div>



                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Last Name</label>
                                    <div class="col-md-9">
                                        <input type="text" class="form-control" id="lname" name="lname">
                                    </div>
                                </div>
                            </div>
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Middle Name</label>
                                    <div class="col-md-9">
                                        <input type="text" id="mname" class="form-control" name="mname">
                                    </div>
                                </div>
                            </div>
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Salutation</label>
                                    <div class="col-md-9">
                                        <input type="text" id="Salutation" class="form-control" name="Salutation">
                                    </div>
                                </div>
                            </div>
                            <div class="form-body">
                                <div class="form-group">
                                    <label class="col-md-3 control-label">Email</label>
                                    <div class="col-md-9">
                                        <input type="email" id="email" class="form-control" name="email">
                                    </div>
                                </div>

                                <input type="hidden" id="IdValue" value="0">


                           
                            </div>
                            <hr>



                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer">
                <button type="button" data-dismiss="modal" class="btn btn-danger">Close</button>
                <button type="button" onclick="Subscription.saveSubscription();" class="btn btn-primary">Save</button>
            </div>

        </div>
    </div>
</div>
@section scripts{
    <script src="~/Resources/theme/js/vendor/datatables/jquery.dataTables.min.js"></script>

    <script src="~/Resources/theme/js/vendor/datatables.net/js/DataTables-1.10.18/js/dataTables.bootstrap.js"></script>
    <script src="~/ScriptsView/Home.js"></script>
    

}